limit 10;

phenotype "Test" version "2";

include ClarityCore version "1.0" called Clarity;

documentset SubmittedDocs:
     Clarity.createDocumentSet({
         "query":"source:%s"});

termset Test:
 ["test","fun"];

define final MyTest:
 Clarity.TermFinder({
  documentset:[SubmittedDocs],
   termset:[Test]
   });
