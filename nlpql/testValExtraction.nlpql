phenotype "Patient Temperatures" version "2";

    include ClarityCore version "1.0" called Clarity;

    documentset SubmittedDocs:
         Clarity.createDocumentSet({
             "query":"source:%s"});

    termset ScoreTerms:
       ["score"];

     define final Scores:
       Clarity.ValueExtraction({
         termset:[ScoreTerms],
         documentset:[SubmittedDocs],
         minimum_value: "0",
         maximum_value: "500"
         });
